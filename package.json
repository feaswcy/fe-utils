{
  "name": "fe-utils",
  "version": "1.0.0",
  "description": "javascript utils using rollup, zero config",
  "main": "src/index.ts",
  "scripts": {
    "dev": "rollup --config build/rollup.base.config.js",
    "build": "rollup --config build/rollup.config.js -f es",
    "build:es": "rollup --config build/rollup.config.js -f es -o dist/utils.esm.js",
    "build:umd": "rollup --config build/rollup.config.js -f umd -0 dist/utils.umd.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "module": "",
  "keywords": [
    "js-utils",
    "rollup"
  ],
  "author": "wangchang3910@qq.com",
  "license": "ISC",
  "devDependencies": {
    "chokidar": "^3.0.0",
    "rollup": "^1.12.4",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-typescript": "^1.0.1",
    "tslib": "^1.9.3",
    "typescript": "^2.9.2"
  },
  "dependencies": {}
}
